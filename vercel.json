{
    "version": 2,
    "builds": [
      {
        "src": "src/index.jsx",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "rewrites": [
        { 
            "src": "/(.*)",
            "dest": "/" 
        }
    ]
}